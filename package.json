{
  "name": "batboxseattle.com",
  "version": "1.0.0",
  "description": "batboxseattle.com",
  "scripts": {
    "build": "concurrently \"npm run build:prepare\" \"npm run build:webpack\"",
    "build:css": "tsx node-scripts/build/css.ts",
    "build:dependencies": "tsx node-scripts/build/dependencies.ts",
    "build:prepare": "npm run build:dependencies && npm run build:css",
    "build:webpack": "webpack --mode production",
    "compress-images": "tsx node-scripts/compress-images.ts",
    "update-collage-images": "tsx node-scripts/update-collage-images.ts",
    "style:check": "tsx node-scripts/style/check.ts",
    "style:fix": "tsx node-scripts/style/fix.ts",
    "start": "concurrently --kill-others \"npm run build:prepare && webpack serve --mode development\" \"npm run watch:css\" \"npm run watch:dependencies\"",
    "watch:css": "chokidar '**/*.css' --ignore 'node_modules/**' --ignore 'dist/**' -c 'npm run build:css'",
    "watch:dependencies": "chokidar 'node_modules/**' -c 'npm run build:dependencies'"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@types/jquery": "^3.5.33",
    "chrono-node": "^2.8.4",
    "date-fns": "^4.1.0",
    "heap-js": "^2.6.0",
    "jquery": "^3.7.1",
    "storage-available": "^1.1.0",
    "swiper": "^11.2.10",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/fs-extra": "^11.0.4",
    "@types/htmlhint": "^1.1.5",
    "@types/node": "^24.3.0",
    "@types/postcss-import": "^14.0.3",
    "@types/puppeteer": "^7.0.4",
    "@types/swiper": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "diff": "^8.0.2",
    "eslint": "^9.34.0",
    "fs-extra": "^11.3.1",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "hint": "^7.1.13",
    "htmlhint": "^1.6.3",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "puppeteer": "^24.17.1",
    "sharp": "^0.34.3",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "uuid": "^11.1.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
